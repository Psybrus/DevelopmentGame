{

	"resources" : [
		{
			"name"		:		"TestEntity",
			"type"		:		"ScnEntity",

			"components" : [
				{
					"$Class"				: "GaMovingComponent",
					"position"				: "0.0, 0.0, 0.0",
					"axisspeed"				: "2.0, 1.0, 0.2",
					"axismagnitude"			: "10.0, 10.0, 10.0"
				},

				{
					"$Class"				: "GaTrailComponent",
					"material"				: "$(ScnMaterial:this.material)",
					"width"					: 1.0,
					"maxlength"				: 30.0,
					"segmentdistance"		: 0.1
				}
			]
		},

		{
			"type"			: "ScnShader",
			"name"			: "shader",
			"sources" 		: {
				"D3D11_4_0" : "Engine/trail.psf",
				"GLSL_430" : "Engine/trail.psf",
				"GLSL_330" : "Engine/trail.psf",
				"GLSL_ES_100" : "Engine/trail.psf"
			},

			"@(android)" : {
				"sources" 		: {
					"GLSL_ES_100" : "Engine/trail.psf"
				}
			},

			"@(html5)" : {
				"sources" 		: {
					"GLSL_ES_100" : "Engine/trail.psf"
				}
			},

			"includepermutations" : [
				"RENDER_FORWARD",
				"RENDER_DEFERRED",
				"MESH_STATIC_3D",
				"PASS_OPAQUE"
			],

			"entrypoints"		: {
				"vertex" : 		"vertexMain",
				"pixel" :		"pixelMain"
			}
		},

		{
			"type"			: "ScnMaterial",
			"name"			: "material",

			// Shader
			"shader"		: "$(ScnShader:this.shader)",

			// Textures
			"textures"		: {
			},

			// Samplers
			"samplers"		: {
			},

			// New render state.
			"renderstate" : {
				"blendstate" : {
					"rendertarget" : [
						{
							"enable" : false,
							"writemask" : 15
						}
					]
				},

				"depthstencilstate" : {
					"depthtestenable" : true,
					"depthwriteenable" : true,
					"depthfunc" : "lessequal",
					"stencilenable" : false
				},

				"rasteriserstate" : {
					"fillmode" : "solid",
					"cullmode" : "none",
					"depthclipenable" : true,
					"antialiasedlineenable" : true
				}
			}
		}
	]
}
