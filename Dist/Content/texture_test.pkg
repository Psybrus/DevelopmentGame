					"font"					: "$(ScnFont:fonts.courier)",
					"material"				: "$(ScnMaterial:materials.font)"
				},

				{
					"$Class"				: "ScnCanvasComponent",
					"noofvertices" 			: 8192,
					"rendermask"			: 1
				},

				{
					"$Class" : "GaTestTextureComponent",
					"material1d" : "$(ScnMaterial:this.material1d)",
					"material2d" : "$(ScnMaterial:this.material2d)",
					"material3d" : "$(ScnMaterial:this.material3d)",
					"materialcube" : "$(ScnMaterial:this.materialcube)"
				}
			]
		},

		{
			"type"			: "ScnMaterial",
			"name"			: "material1d",

			// Shader
			"shader"		: "$(ScnShader:this.texture_test_1d)",

			// Textures
			"textures"		: {
			},

			// Samplers
			"samplers"		: {
				"aDiffuseTex"	: {
					"addressu" : "wrap",
					"minfilter" : "linear",
					"magfilter" : "linear"
				}
			},

			// New render state.
			"renderstate" : {
				"blendstate" : {
					"rendertarget" : [
						{
							"enable" : false,
							"writemask" : 15
						}
					]
				},

				"depthstencilstate" : {
					"depthtestenable" : false,
					"depthwriteenable" : true,
					"depthfunc" : "lessequal",
					"stencilenable" : false
				},

				"rasteriserstate" : {
					"fillmode" : "solid",
					"cullmode" : "none",
					"depthclipenable" : true,
					"antialiasedlineenable" : false
				}
			}
		},

