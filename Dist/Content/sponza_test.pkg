						{
							"$Class"				: "GaTestParticleComponent"
						}
						*/
					]
				}

			]
		},

		{
			"type"		: "ScnModel",
			"name"		: "sponza",
			"source"	: "Source/crytek-sponza/sponza.obj",

			"flattenhierarchy" : true,

			"materials": [
				{
					"regex" : "(.*)", 
					"templatematerial" : {
						"$Class"		: "ScnMaterialImport",
						"shader"		: "$(ScnShader:shaders.textured_normal_metallic_roughness)",

						"renderstate" : {
							"blendstate" : {
								"rendertarget" : [
									{
										"enable" : false
									}
								]
							},

							"depthstencilstate" : {
								"depthtestenable" : true,
								"depthwriteenable" : true,
								"depthfunc" : "lessequal",
								"stencilenable" : false
							},

							"rasteriserstate" : {
								"fillmode" : "solid",
								"cullmode" : "ccw",
								"depthclipenable" : true,
								"antialiasedlineenable" : true
							}
						},

						"automaticuniformblocks" : [
							"ScnShaderMaterialUniformBlockData"
						]

					}
				}

				/*
				,

				{
					"regex" : "chain",
					"material" : "$(ScnMaterial:this.chain)"
				},

				{
					"regex" : "Material__57",
					"material" : "$(ScnMaterial:this.Material__57)"
				},

				{
					"regex" : "leaf",
					"material" : "$(ScnMaterial:this.leaf)"
				}
				*/
			],

			"vertexformat" : {
				"position" : "FLOAT32",
				"normal" : "FLOAT32",
				"texcoord0" : "FLOAT32",
				"tangent" : "FLOAT32",
				"binormal" : "FLOAT32"
			},

			"defaulttextures" : {
				"aDiffuseTex" : "$(ScnTexture:textures.white)",
				"aNormalTex" : "$(ScnTexture:textures.normal)",
				"aSpecularTex" : "$(ScnTexture:textures.green)",
				"aOpacityTex" : "$(ScnTexture:textures.white)"
			}
		}

